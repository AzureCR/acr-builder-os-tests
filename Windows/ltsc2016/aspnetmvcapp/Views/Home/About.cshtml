@{
    ViewBag.Title = "About";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<h3>Host Information</h3>
<b>OS Architecture:</b> @ViewData["OSARCHITECTURE"]<br />
<b>OS Description:</b> @ViewData["OSDESCRIPTION"]<br />
<b>Process Architecture:</b> @ViewData["PROCESSARCHITECTURE"]<br />
<b>Processor Revision</b>@ViewData["PROCESSOR_REVISION"]<br />
<b>Framework Description:</b> @ViewData["FRAMEWORKDESCRIPTION"]<br />

<h3>Environment Variables:</h3>
@Html.Raw(ViewData["ENV_VARS"])